{
  "name": "klaviyo-ct-plugin",
  "version": "1.0.0",
  "description": "Klaviyo plugin for integration with commercetools",
  "main": "index.js",
  "author": "E2X",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run clean && ./build.js && cp package.json ./dist/package.json",
    "clean": "rm -rf dist/",
    "klaviyo-types": "dts-gen -m klaviyo-api -f src/types/klaviyo-api.d.ts -o",
    "lint": "eslint 'src/**/*.ts' --quiet",
    "lint:fix": "eslint 'src/**/*.ts' --quiet --fix",
    "start": "ts-node-esm src/index.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:watch": "npm test -- --watch",
    "ts-types": " tsc --emitDeclarationOnly --outDir dist",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commercetools/platform-sdk": "^4.3.0",
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "dts-gen": "^0.6.1",
    "esbuild": "^0.16.13",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "nock": "^13.3.0",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@commercetools/sdk-client-v2": "^2.1.1",
    "@google-cloud/logging-winston": "^5.3.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "klaviyo-api": "^1.0.2",
    "winston": "^3.8.2"
  }
}
